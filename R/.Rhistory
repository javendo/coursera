complete <- function(directory, id = 1:332) {
  ## 'directory' is a character vector of length 1 indicating
  ## the location of the CSV files

  ## 'id' is an integer vector indicating the monitor ID numbers
  ## to be used

  ## Return a data frame of the form:
  ## id nobs
  ## 1  117
  ## 2  1041
  ## ...
  ## where 'id' is the monitor ID number and 'nobs' is the
  ## number of complete cases

  table.all <- data.frame(id=numeric(), nobs=numeric())
  for (f in list.files(path = directory)) {
    file <- paste(directory, "/", f, sep="")
    table <- read.csv(file)
    #table.na.omit <- na.omit(table)
    if (length(table.na.omit$ID) > 0)
      table.all <- rbind(table.all, aggregate(list(nobs=table.na.omit$ID), list(id=table.na.omit$ID), length))
    #print(nrow(table[complete.cases(table)))
    #print(length(table[complete.cases(table),]$Date))
  }
  table.all
}
